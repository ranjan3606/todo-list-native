{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease",
        "image": "ubuntu-18.04-jdk-11-ndk-r19c"
      },
      "ios": {
        "image": "latest",
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}